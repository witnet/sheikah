{
  "name": "sheikah-witnet-wallet",
  "productName": "Sheikah Witnet wallet",
  "version": "1.5.0",
  "private": true,
  "description": "A Witnet compatible desktop wallet interface and data requests development environment.",
  "author": "Witnet Foundation <info@witnet.foundation>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --config=jest.unit.config.js",
    "ci": "yarn lint:check && yarn test && yarn electron:build --publish=never",
    "clean": "yarn clean-deps && rm -rf dist_electron",
    "clean-deps": "rm -rf node_modules",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:serve:wallet": "vue-cli-service electron:serve --wallet",
    "lint:eslint": "eslint",
    "lint:stylelint": "stylelint",
    "lint:prettier": "prettier --loglevel warn",
    "lint:fix:eslint": "yarn lint:eslint --fix --ext .js,.vue .",
    "lint:fix:stylelint": "yarn lint:stylelint --fix \"src/**/*.{vue,scss}\"",
    "lint:fix:prettier": "yarn lint:prettier --write \"**/*.{js,json,css,scss,vue,html}\"",
    "lint:check:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:check:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:check:prettier": "yarn lint:prettier --check \"**/*.{js,json,css,scss,vue,html}\"",
    "lint:check": "yarn lint:check:eslint && yarn lint:check:prettier && yarn lint:check:stylelint",
    "lint": "yarn lint:fix:eslint && yarn lint:fix:stylelint && yarn lint:fix:prettier",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "reinstall": "yarn clean-deps && yarn",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "test": "yarn test:unit",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "axios": "^0.19.1",
    "big.js": "^5.2.2",
    "cbor": "^4.1.5",
    "commander": "^6.2.0",
    "core-js": "^3.4.4",
    "date-fns": "^2.12.0",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.5",
    "element-ui": "^2.4.5",
    "fs-extra": "^9.0.1",
    "progress-stream": "^2.0.0",
    "rpc-websockets": "^4.3.5",
    "tar": "^6.0.1",
    "tree-kill": "^1.2.2",
    "uuid": "^3.3.2",
    "vue": "^2.6.11",
    "vue-json-tree": "^0.4.3",
    "vue-observe-visibility": "^0.4.6",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "witnet-radon-js": "0.8.1"
  },
  "devDependencies": {
    "@babel/compat-data": "7.9.0",
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-eslint": "4.2.x",
    "@vue/cli-plugin-unit-jest": "^4.2.2",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "6.0.x",
    "@vue/eslint-config-standard": "5.1.x",
    "@vue/test-utils": "1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^7.0.2",
    "electron": "11.0.3",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^1.0.2",
    "eslint": "6.8.x",
    "eslint-plugin-import": "2.20.x",
    "eslint-plugin-node": "11.0.x",
    "eslint-plugin-promise": "4.2.x",
    "eslint-plugin-standard": "4.0.x",
    "eslint-plugin-vue": "6.2.x",
    "lint-staged": "^10.2.7",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "stylelint": "13.2.x",
    "stylelint-config-css-modules": "2.2.x",
    "stylelint-config-prettier": "8.0.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "20.0.x",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "3.14.x",
    "vue-cli-plugin-electron-builder": "2.0.0-rc.4",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-cli-plugin-styleguidist": "^4.17.0",
    "vue-template-compiler": "^2.5.21"
  },
  "bugs": {
    "url": "https://github.com/witnet/sheikah/issues"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/witnet/sheikah.git"
  }
}
